{
  "name": "msonowal/laravel-tinify",
  "type": "library",
  "description": "Laravel Facade for tinify API",
  "keywords": ["tinify","tinypng"],
  "license": "MIT",
  "authors": [
    {
      "name": "Manash Jyoti Sonowal",
      "email": "manash149@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.x",
    "tinify/tinify":"~1.1"
  },
  "autoload": {
    "psr-4": {
      "msonowal\\LaravelTinify\\": "src"
    }
  },
  "minimum-stability": "stable",
  "extra": {
      "laravel": {
          "providers": [
              "msonowal\\LaravelTinify\\LaravelTinifyServiceProvider"
          ],
          "aliases": {
              "Tinify": "msonowal\\LaravelTinify\\Facades\\Tinify"
          }
      }
    }
}
